{
  "name": "eth-event-subscriptions",
  "version": "0.0.0-development",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "engines": {
    "node": "> 14"
  },
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn clean && yarn type:typechain && yarn rollup",
    "clean": "rm -rf dist",
    "type:typechain": "rm -rf src/contracts && typechain --target=web3-v1 --out-dir=./src/contracts ./src/abi/*.json",
    "type:check": "tsc --noEmit",
    "rollup": "rollup --config ./rollup.config.ts --configPlugin rollup-plugin-typescript2",
    "format": "prettier 'src/**/*.ts' 'tests/**/*.ts' 'examples/**/*.ts' --write",
    "format:check": "prettier 'src/**/*.ts' 'tests/**/*.ts' --check",
    "lint": "eslint . --ext .ts --fix",
    "lint:check": "eslint . --ext .ts",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' ts-mocha -r esm -p tsconfig.json tests/**/*.ts",
    "spell:check": "cspell \"{README.md,CODE_OF_CONDUCT.md,CONTRIBUTING.md,.github/*.md,src/**/*.ts,tests/**/*.ts}\"",
    "cz": "cz",
    "semantic-release": "semantic-release",
    "docs": "md-magic --path './README.md'",
    "prepare": "husky install",
    "start": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' ts-node -r esm ./src/index.ts"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-inject": "^4.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "@typechain/web3-v1": "^5.0.0",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.0.0",
    "@types/prompts": "^2.0.13",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "chai": "^4.3.4",
    "commitizen": "^4.2.4",
    "cspell": "^5.6.6",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.23.4",
    "esm": "^3.2.25",
    "husky": "^7.0.0",
    "markdown-magic": "^2.5.2",
    "mocha": "^9.1.1",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "rollup": "^2.66.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.30.0",
    "semantic-release": "^17.4.4",
    "source-map-support": "^0.5.19",
    "ts-mocha": "^9.0.2",
    "typechain": "^7.0.0",
    "typescript": "^4.3.5"
  },
  "volta": {
    "node": "16.4.1",
    "yarn": "1.22.10",
    "npm": "7.19.1"
  },
  "dependencies": {
    "@alch/alchemy-web3": "^1.1.11",
    "axios": "^0.22.0",
    "bufferutil": "^4.0.4",
    "parquetjs": "^0.11.2",
    "utf-8-validate": "^5.0.6",
    "@google-cloud/storage": "^5.3.0"
  }
}